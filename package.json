{
  "name": "next-auth-prisma-stripe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:ci": "yarn prisma:generate && yarn prisma:migrate:prod && yarn build",
    "prisma:migrate:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate deploy",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:seed": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma db seed",
    "stripe:dev": "stripe listen --forward-to  http://localhost:3000/api/stripe/webhook",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "db-seed": "tsc prisma/seed.ts && node prisma/seed.js | node --input-type=\"commonjs\" && rm prisma/seed.js",
    "db-push": "prisma db push "
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.7.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@mui/lab": "^5.0.0-alpha.70",
    "@mui/material": "^5.4.3",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@nextui-org/react": "^1.0.0-beta.12",
    "@popperjs/core": "^2.11.2",
    "@prisma/client": "^4.13.0",
    "@stripe/stripe-js": "^1.17.1",
    "@toast-ui/editor": "^3.2.2",
    "apexcharts": "^3.40.0",
    "apexcharts-clevision": "3.28.5",
    "axios": "^1.4.0",
    "babel-plugin-superjson-next": "^0.4.1",
    "bcrypt": "^5.1.0",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^4.0.0",
    "google-trends-api": "^4.9.2",
    "mdi-material-ui": "^7.1.0",
    "next": "^12.0.4",
    "next-auth": "^4.22.1",
    "next-connect": "^0.10.2",
    "next-themes": "^0.2.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.6.3",
    "nprogress": "^0.2.0",
    "openai": "^3.2.1",
    "react": "17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-datepicker": "^4.5.0",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.4.1",
    "react-perfect-scrollbar": "^1.5.8",
    "react-popper": "^2.2.5",
    "reactflow": "^11.7.0",
    "stripe": "^8.176.0",
    "superjson": "^1.7.5",
    "sweetalert2": "^11.7.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.16.3",
    "@types/node-cron": "^3.0.7",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.21",
    "@types/react-datepicker": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.3.4",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "next-transpile-modules": "^9.0.0",
    "postcss": "^8.3.6",
    "prisma": "^4.13.0",
    "tailwindcss": "^2.2.15",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.3"
  }
}
